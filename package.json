{
  "name": "@blankapp/ui",
  "version": "1.0.4",
  "description": "Highly customizable and theming components for React Native",
  "main": "index.js",
  "repository": "git@github.com:blankapp/ui.git",
  "keywords": [
    "react-native",
    "ios",
    "android",
    "components",
    "native-components",
    "ui-components"
  ],
  "author": "JianyingLi <lijy91@foxmail.com>",
  "license": "MIT",
  "pre-commit": [
    "lint",
    "test"
  ],
  "scripts": {
    "lint": "node node_modules/eslint/bin/eslint.js src/**",
    "test": "jest"
  },
  "dependencies": {
    "hoist-non-react-statics": "^2.3.1",
    "lodash": "^4.17.4"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.3",
    "babel-jest": "21.2.0",
    "babel-preset-react-native": "4.0.0",
    "eslint": "^4.13.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.5.1",
    "jest": "^21.2.1",
    "pre-commit": "^1.2.2",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-native": "^0.51.0",
    "react-test-renderer": "16.2.0"
  },
  "peerDependencies": {
    "prop-types": "*",
    "react": "*",
    "react-native": "*"
  },
  "jest": {
    "notify": true,
    "preset": "react-native",
    "testRegex": "./src/.*\\-test\\.js$",
    "setupFiles": [
      "<rootDir>/jest-setup.js"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/examples/"
    ]
  }
}
